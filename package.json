{
  "name": "stock-controll",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "v20.10.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "build": "pnpm prisma:generate && pnpm prisma:migrate && next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "prisma:migrate": "prisma migrate deploy --schema=./app/api/_backend/prisma/resource/schema.prisma",
    "prisma:generate": "prisma generate --schema=./app/api/_backend/prisma/resource/schema.prisma",
    "seed": "tsx app/api/_backend/prisma/resource/seed.ts",
    "postinstall": "pnpm prisma:generate"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.2.0",
    "@mui/material": "^6.2.0",
    "@prisma/client": "^6.7.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.412.0",
    "moment": "^2.30.1",
    "next": "15.0.4",
    "next-auth": "5.0.0-beta.27",
    "next-barcode": "^1.5.0",
    "next-pwa": "^5.6.0",
    "next-qrcode": "^2.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.0",
    "react-media-devices": "^1.1.5",
    "react-toastify": "^10.0.6",
    "react-zxing": "^2.0.2",
    "redoc": "^2.5.0",
    "sweetalert2": "^11.15.3",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animated": "^2.0.0",
    "tsx": "^4.19.2",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "yup": "^1.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/moment": "^2.13.0",
    "@types/ms": "^2.1.0",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.4",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "overrides": {
    "react-zxing": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-media-devices": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}